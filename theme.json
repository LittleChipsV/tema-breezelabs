{
  "$schema": "https://schemas.wp.org/trunk/theme.json",
  "version": 3,
  "settings": {
    "appearanceTools": true,
    "layout": {
      "contentSize": "1120px",
      "wideSize": "1440px"
    },
    "border": {
      "radius": true,
      "color": true,
      "style": true,
      "width": true
    },
    "custom": {
      "borderRadius": {
        "none": "0",
        "small": "0.25em",
        "medium": "0.5em",
        "large": "1em",
        "round": "50%"
      }
    },
    "color": {
      "background": true,
      "custom": true,
      "customDuotone": true,
      "customGradient": true,
      "defaultGradients": true,
      "defaultPalette": false,
      "palette": [
        { 
          "name": "Primary",
          "slug": "primary",
          "color": "#214D4D"
        },
        { 
          "name": "Secondary",
          "slug": "secondary",
          "color": "#379591"
        }, 
        { 
          "name": "Accent",
          "slug": "accent",
          "color": "#e44f4f"
        }, 
        { 
          "name": "Success",
          "slug": "success",
          "color": "#379591"
        }, 
        { 
          "name": "Info",
          "slug": "info",
          "color": "#4986e8"
        }, 
        { 
          "name": "Warning",
          "slug": "warning",
          "color": "#d5a81d"
        }, 
        { 
          "name": "Background",
          "slug": "background",
          "color": "#fafafa"
        }, 
        { 
          "name": "Dark",
          "slug": "dark",
          "color": "#0d2526"
        }, 
        { 
          "name": "Light",
          "slug": "light",
          "color": "#f3faf9"
        }, 
        { 
          "name": "Pure Black",
          "slug": "pure-black",
          "color": "#000000"
        },
        { 
          "name": "Pure White",
          "slug": "pure-white",
          "color": "#ffffff"
        },
        { 
          "name": "Transparent",
          "slug": "transparent",
          "color": "rgba(255, 255, 255, 0)"
        }
      ]
    },
    "spacing": {
      "customSpacingSize": true,
      "defaultSpacingSizes": false,
      "units": [
        "px",
        "em",
        "rem",
        "vh",
        "vw",
        "%"
      ],
      "spacingSizes": [
        {
          "name": "X-Small",
          "size": "0.25rem",
          "slug": "10"
        },
        {
          "name": "Small",
          "size": "0.5rem",
          "slug": "20"
        },
        {
          "name": "Medium",
          "size": "1rem",
          "slug": "30"
        },
        {
          "name": "Large",
          "size": "1.5rem",
          "slug": "40"
        },
        {
          "name": "X-Large",
          "size": "2rem",
          "slug": "50"
        },
        {
          "name": "2X-Large",
          "size": "3rem",
          "slug": "60"
        },
        {
          "name": "3X-Large",
          "size": "4rem",
          "slug": "70"
        }
      ]
    },
    "typography": {
      "customFontSize": true,
      "textAlign": true,
      "writingMode": true,
      "dropCap": true,
      "fluid": true,
      "fontStyle": true,
      "fontWeight": true,
      "letterSpacing": true,
      "textDecoration": true,
      "textTransform": true,
      "defaultFontSizes": false,
      "fontFamilies": [
        {
          "fontFamily": "-apple-system, BlinkMacSystemFont, \"Segoe UI\", Roboto, \"Helvetica Neue\", Arial, sans-serif, \"Apple Color Emoji\", \"Segoe UI Emoji\", \"Segoe UI Symbol\"",
          "slug": "system-font",
          "name": "System Font"
        },
        {
          "name": "Open Sans, sans-serif",
          "slug": "open-sans-sans-serif",
          "fontFamily": "Open Sans, sans-serif",
          "fontFace": [
            {
              "fontFamily": "Open Sans",
              "fontWeight": "300 800",
              "fontStyle": "normal",
              "fontStretch": "normal",
              "src": [ "file:./assets/fonts/Open_Sans/OpenSans.ttf" ]
            },
            {
              "fontFamily": "Open Sans",
              "fontWeight": "300 800",
              "fontStyle": "italic",
              "fontStretch": "normal",
              "src": [ "file:./assets/fonts/Open_Sans/OpenSans-Italic.ttf" ]
            }
          ]
        }
      ], 
      "fontSizes": [
        {
          "slug": "small",
          "size": "clamp(0.875rem, 0.875rem + 0.2vw, 1rem)",
          "name": "Small"
        },
        {
          "slug": "body",
          "size": "clamp(1rem, 1rem + 0.5vw, 1.125rem)",
          "name": "Body"
        },
        {
          "slug": "lead",
          "size": "clamp(1.125rem, 1.125rem + 0.5vw, 1.25rem)",
          "name": "Lead"
        },
        {
          "slug": "h6",
          "size": "clamp(1rem, 1rem + 0.5vw, 1.25rem)",
          "name": "H6"
        },
        {
          "slug": "h5",
          "size": "clamp(1.25rem, 1.25rem + 0.5vw, 1.5rem)",
          "name": "H5"
        },
        {
          "slug": "h4",
          "size": "clamp(1.5rem, 1.5rem + 0.5vw, 1.75rem)",
          "name": "H4"
        },
        {
          "slug": "h3",
          "size": "clamp(1.75rem, 1.75rem + 0.5vw, 2rem)",
          "name": "H3"
        },
        {
          "slug": "h2",
          "size": "clamp(2rem, 2rem + 0.5vw, 2.25rem)",
          "name": "H2"
        },
        {
          "slug": "h1",
          "size": "clamp(2.25rem, 2.25rem + 0.5vw, 3rem)",
          "name": "H1"
        }
      ]
    }
  },
  "styles": {
    "typography": {
      "fontSize": "var(--wp--preset--font-size--body)",
      "fontFamily": "var(--wp--preset--font-family--open-sans-sans-serif)",
      "lineHeight": "1.6"
    },
    "color": {
      "background": "var(--wp--preset--color--background)",
      "text": "var(--wp--preset--color--dark)"
    },
    "blocks": {
      "core/site-title": {
        "typography": {
          "fontWeight": "bold",
          "fontSize": "var(--wp--preset--font-size--h-3)"
        }
      },
      "core/button": {
        "border": {
          "radius": "var(--wp--custom--border-radius--none)"
        }
      }
    },
    "elements": {
      "h1": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--h-1)",
          "lineHeight": "1.2"
        }
      },
      "h2": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--h-2)",
          "lineHeight": "1.3"
        }
      },
      "h3": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--h-3)",
          "lineHeight": "1.4"
        }
      },
      "h4": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--h-4)",
          "lineHeight": "1.4"
        }
      },
      "h5": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--h-5)",
          "lineHeight": "1.5"
        }
      },
      "h6": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--h-6)",
          "lineHeight": "1.5"
        }
      }
    }
  },
  "templateParts": [
    {
      "name": "header",
      "title": "Header",
      "area": "header"
    },
    {
      "name": "footer",
      "title": "Footer",
      "area": "footer"
    }
  ]
}
